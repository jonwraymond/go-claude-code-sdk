{
  "ignorePatterns": [
    {
      "pattern": "^https://github.com/.*/(pull|issues)/[0-9]+$"
    },
    {
      "pattern": "^https://pkg.go.dev/"
    }
  ],
  "replacementPatterns": [
    {
      "pattern": "^/",
      "replacement": "https://github.com/jonwraymond/go-claude-code-sdk/blob/main/"
    }
  ],
  "httpHeaders": [
    {
      "urls": ["https://github.com/", "https://raw.githubusercontent.com/"],
      "headers": {
        "Accept": "text/html"
      }
    }
  ],
  "timeout": "10s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackRetryDelay": "5s"
}